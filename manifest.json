{
  "manifest_version": 3,
  "name": "IUH Calendar Sync",
  "version": "1.0",
  "description": "Sync IUH schedule to Google Calendar",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://sv.iuh.edu.vn/*"],
  "content_scripts": [
    {
      "matches": ["https://sv.iuh.edu.vn/*"],
      "js": ["content.js"]
    }
  ]
}
