{
  "manifest_version": 3,
  "name": "IUH Calendar Sync",
  "version": "2.0",
  "permissions": [
    "activeTab", 
    "scripting", 
    "storage"  
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://sv.iuh.edu.vn/*"],
      "js": ["content.js"]
    }
  ]
}